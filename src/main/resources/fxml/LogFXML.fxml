<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane fx:id="anchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.chenum.controller.LogFXMLController">
   <children>
      <TextArea fx:id="consArea" layoutY="22.0" prefHeight="381.0" prefWidth="600.0"/>
      <HBox prefHeight="100.0" prefWidth="600.0">
         <children>
            <RadioButton fx:id="infoBtn" mnemonicParsing="false" text="info" />
            <RadioButton fx:id="warnBtn" mnemonicParsing="false" text="warn" />
            <RadioButton fx:id="debugBtn" mnemonicParsing="false" text="debug" />
            <RadioButton fx:id="errorBtn" mnemonicParsing="false" text="error" />
            <RadioButton fx:id="traceBtn" mnemonicParsing="false" text="trace" />
         </children>
      </HBox>
   </children>
</AnchorPane>
